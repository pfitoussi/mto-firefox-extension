{
  "manifest_version": 2,
  "name": "mto",
  "description": "description",
  "version": "2.0.0",
  "icons": {
    "48": "icons/shape-line-48.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "icons/shape-line-48.png"
    },
    "default_popup": "index.html",
    "default_title": "mto"
  },
  "background": {
    "page": "background.html"
  },

  "permissions": [
      "activeTab",
      "contextMenus",
      "storage"
  ]
}